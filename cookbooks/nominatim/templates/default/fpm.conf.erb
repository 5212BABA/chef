[<%= @name %>]
listen = /var/run/php5-fpm-<%= @name %>.sock

user = www-data
group = www-data

pm = <%= @pm %>
pm.max_children = <%= @max_children %>
pm.start_servers = 20
pm.min_spare_servers = 20
pm.max_spare_servers = 30
pm.max_requests = 1000000

security.limit_extensions = .php .phpx .phpj
