# DO NOT EDIT - This file is being maintained by Chef

/var/log/apache2/*.log {
  weekly
  missingok
  rotate 8
  compress
  delaycompress
  notifempty
  create 640 root adm
  sharedscripts
  postrotate
    if /etc/init.d/apache2 status > /dev/null ; then \
        /etc/init.d/apache2 reload > /dev/null; \
    fi;
  endscript
}
