# DO NOT EDIT - This file is being maintained by Chef

/var/log/apache2/*.log {
  daily
  missingok
  rotate 42
  compress
  delaycompress
  notifempty
  create 640 root adm
  sharedscripts
  postrotate
    /bin/systemctl reload apache2
  endscript
}


